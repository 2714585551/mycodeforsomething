package com.lpp.mq.core.enums;

/**
  * @ClassName: QuerySqlTypeEnum
  * @FullClassPath: com.lpp.mq.core.QuerySqlTypeEnum
  * @Description: 查询sql类型
  * @author: Arno
  * @date: 2017年3月8日 上午10:38:04
  * @version: 1.0
  */

public enum QuerySqlTypeEnum {
	
	IN("IN"),
	AND("AND"),
	EQUAL("="),
	NOTEQUAL("<>"),
	ALT("<"),
	GLT(">"),
	ASC("ASC"),
	LIKE("LIKE"),
	DESC("DESC"),
	ORDER_BY("ORDER BY");
	
	private String type ;

	/**
	 * 
	 */
	private QuerySqlTypeEnum() {

	}
	
	private QuerySqlTypeEnum(String type) {
		this.type = type;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}
	
}
