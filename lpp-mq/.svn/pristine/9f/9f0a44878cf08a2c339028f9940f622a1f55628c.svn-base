public enum QuerySqlTypeEnum {
	
	IN("in"),
	AND("and"),
	ASC("asc"),	DESC("desc"),
	LIKE("like"),
	ORDER_BY("order by");
	
	private String type ;

	/**
	 * 
	 */
	private QuerySqlTypeEnum() {

	}
	
	private QuerySqlTypeEnum(String type) {
		this.type = type;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}
	
}
